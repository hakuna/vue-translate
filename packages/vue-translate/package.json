{
  "name": "vue-translate",
  "version": "0.1.0",
  "description": "Internationalization plugin for Vue.js",
  "keywords": [
    "vue",
    "i18n",
    "internationalization",
    "translation",
    "localization"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.0",
    "jsdom": "^23.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.3",
    "vitest": "^0.34.0",
    "@vue/test-utils": "^2.4.0",
    "vue": "^3.0.0"
  }
}
