{
  "name": "@hakunajs/unplugin-vue-translate",
  "version": "0.0.2",
  "description": "Internationalization plugin for Vue.js that supports SFC i18n blocks",
  "keywords": [
    "vue",
    "i18n",
    "internationalization",
    "translation",
    "localization",
    "vite",
    "webpack",
    "rollup"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vite": "^5.0.0",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@vitejs/plugin-vue": "^5.2.3",
    "unplugin": "^1.4.0",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.33.0",
    "@vue/compiler-sfc": "^3.3.4",
    "tsup": "^7.1.0",
    "typescript": "^5.1.3",
    "vitest": "^0.33.0",
    "vue": "^3.3.4",
    "vue-loader": "^17.2.2",
    "webpack": "^5.88.2"
  }
}
